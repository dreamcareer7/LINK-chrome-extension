{
  "manifest_version": 2,
  "name": "Jayla",
  "version": "1.0",
  "description": "LinkedIn Lead Generation Management & CRM Software",
  "web_accessible_resources": [
    "img/*.svg"
  ],
  "background": {
    "scripts": [
      "util.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "matches": [
        "https://www.linkedin.com/*"
      ]
    }
  ],
  "permissions": [
    "storage",
    "cookies",
    "webNavigation",
    "webRequest",
    "https://www.linkedin.com/*",
    "https://link.dev.gradlesol.com/app/*",
    "https://link.dev.humanpixel.com.au/app/*",
    "https://link.test.humanpixel.com.au/app/*",
    "https://jayla.ai/app/*",
    "https://jayla.linkfluencer.com/app/*"
  ],
  "browser_action": {
    "default_icon": "img/defaultIcon.png",
    "default_popup": "signin.html"
  }
}
